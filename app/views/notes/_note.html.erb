<% track.notes.each do |note| %>
<li>
  <%= note.note %> - <%= note.user.email %>
  <% if note.user_id == current_user.id %>
    <form method="POST" action="<%= note_url(note.id) %>">
      <%= auth_token %>
      <input type="hidden" name="params" value="test">
      <input type="hidden" name="_method" value="DELETE">
      <button value="submit">delete note</button>
    </form>
  <% end %>
</li>
<% end %>
